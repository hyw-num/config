expect <<END     
    set timeout 300
    set user [lindex $argv 1]
    set pass [lindex $argv 2]
    set repo [lindex $argv 3]
    puts "$repo"
    puts "$user"
    puts "$pass"
    spawn git push "$3" main
    expect "Username for 'https://github.com': "
    send "$user\r"
    expect "Password for 'https://github.com': "
    send "$pass\r"
END
